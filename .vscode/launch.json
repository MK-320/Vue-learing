{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Chrome Instance",
            "type": "chrome",
            "request": "launch",
            "url": "http://127.0.0.1:8080/#/",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
  }